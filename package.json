    "dotenv": "^16.0.3",
    "pg": "^8.10.0",
    "sequelize": "^6.35.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.10.0",
    "axios": "^1.6.0",
    "uuid": "^9.0.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.50.0",
    "prettier": "^3.0.3"
  }
}
